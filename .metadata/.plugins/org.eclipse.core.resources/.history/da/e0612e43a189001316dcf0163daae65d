/**
 template for frame
 */
import javax.swing.JFrame;
/**
 * FaceTester
 * @author szn2385
 * CS220 Java
 * Lab 1
 */
public class FaceTester {
    //Method that constructs the frame
	public static void main(String[] args)
	{
		/**
		 * Declares the dimensions the frame which has been named table
		 */
		JFrame table = new JFrame();
		
		table.setSize(300, 200);
		table.setTitle("Luck of the Roll");
		table.setDefaultCloseOperation(JFrame.EXIT_ON_CLOSE);
		
		//Commands a variable named rollone of the class MyFace to be drawn and made visible on table
		MyFace rollone = new MyFace(4,5);
	    table.add(rollone);
	    table.setVisible(true); 
	    
	  //Commands a variable named rollsix of the class OppositeFace to be drawn and made visible on table
	  	OppositeFace rollsix = new OppositeFace(1,1);
	  	table.add(rollsix);
	  	table.setVisible(true);
	  	
	 
		
	}
	
}
